{
    "name": "oberonamsterdam/tor-graphql-client",
    "description": "GraphQL library to communicate with TOR",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Oberon",
            "email": "contact@oberon.nl"
        },
        {
            "name": "Raymond Kiekens",
            "email": "raymond@oberon.nl"
        }
    ],
    "require": {
        "php": "^7.2",
        "gmostafa/php-graphql-client": "^1.4",
        "phpdocumentor/reflection-docblock": "^5.1",
        "symfony/property-info": "^4.3",
        "symfony/serializer": "^4.3"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.2"
        }
    },
    "autoload": {
        "psr-4": {
            "TOR\\GraphQL\\": "src/"
        }
    }
}
